{
    "name": "benzed-ts",
    "description": "TypeScript mono repo for all BenZed's code.",
    "scripts": {
        "update-deps": "npx npm-check -u",
        "postinstall": "npm run packages:bootstrap",
        "packages:bootstrap": "lerna bootstrap --force-local",
        "packages:build": "lerna run build",
        "packages:develop": "ts-node ./scripts/packages-develop.ts",
        "packages:test": "lerna run test",
        "packages:tsc": "lerna run tsc",
        "packages:lint": "lerna run lint",
        "packages:publish": "npm run packages:sync-versions; ts-node ./scripts/packages-publish.ts",
        "packages:sync-versions": "ts-node ./scripts/packages-sync-versions.ts",
        "start:file-server": "node ./packages/file-server/lib/scripts/start-file-server.js",
        "start:client-renderer": "node ./packages/file-server/lib/scripts/start-client-renderer.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/BenZed/benzed-ts.git"
    },
    "author": "BenZed",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/BenZed/benzed-ts/issues"
    },
    "homepage": "https://github.com/BenZed/benzed-ts#readme",
    "devDependencies": {
        "@types/compression": "^1.7.2",
        "@types/jest": "^29.2.0",
        "@types/node": "^18.11.7",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "@types/semver": "^7.3.10",
        "@typescript-eslint/eslint-plugin": "^5.40.1",
        "@typescript-eslint/parser": "^5.40.1",
        "babel-jest": "^29.5.0",
        "chokidar": "^3.5.3",
        "clean-webpack-plugin": "^4.0.0",
        "cross-fetch": "^3.1.5",
        "css-loader": "^6.7.3",
        "eslint": "^8.26.0",
        "expect-type": "^0.15.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.5.0",
        "jest": "^29.2.1",
        "lerna": "^6.0.1",
        "mini-css-extract-plugin": "^2.7.3",
        "normalize.css": "^8.0.1",
        "semver": "^7.3.8",
        "shx": "^0.3.4",
        "ts-jest": "^29.0.3",
        "ts-loader": "^9.4.2",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4",
        "typescript-plugin-styled-components": "^2.0.0",
        "webpack": "^5.76.1",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^4.11.1"
    },
    "peerDependencies": {
        "mongodb": "^4.11.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.9.0"
    }
}
