{
    "name": "@benzed/file-server",
    "description": "A quick-setup file server incorperating @file-service and @renderer packages",
    "main": "./lib",
    "version": "1.0.0",
    "scripts": {
        "test": "NODE_ENV=test jest --maxWorkers=1 -o",
        "test:dev": "npm run test -- --verbose --watch",
        "build": "npm run lint && npm run tsc",
        "lint": "eslint src/**/*.ts --fix",
        "tsc": "shx rm -rf lib && shx mkdir lib && tsc"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/BenZed/benzed-ts.git"
    },
    "author": "BenZed",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/BenZed/benzed-ts/issues"
    },
    "homepage": "https://github.com/BenZed/benzed-ts#readme",
    "dependencies": {
        "@benzed/array": "^3.1.4",
        "@benzed/is": "^3.1.2",
        "@benzed/math": "^3.1.4",
        "@benzed/util": "^3.7.0",
        "@benzed/feathers": "^1.0.0",
        "ajv-errors": "^3.0.0"
    },
    "peerDependencies": {
        "@feathersjs/authentication": "^5.0.0-pre.27",
        "@feathersjs/authentication-local": "^5.0.0-pre.27",
        "@feathersjs/configuration": "^5.0.0-pre.27",
        "@feathersjs/express": "5.0.0-pre.27",
        "@feathersjs/feathers": "^5.0.0-pre.27",
        "@feathersjs/socketio": "^5.0.0-pre.27",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "helmet": "^5.0.2"
    },
    "devDependencies": {
        "@types/compression": "^1.7.2"
    }
}