    },
    "homepage": "https://github.com/BenZed/benzed-ts#readme",
    "dependencies": {
        "@benzed/is": "*",
        "@benzed/array": "*",
        "@benzed/math": "*",
        "@benzed/util": "*",
        "fluent-ffmpeg": "^2.1.2"
    },
    "devDependencies": {
        "@types/fluent-ffmpeg": "^2.1.20"
