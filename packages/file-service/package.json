{
    "name": "@benzed/file-service",
    "description": "File Service and and server incorperating @feathers & @renderer package",
    "main": "./lib",
    "version": "1.0.0",
    "scripts": {
        "test": "jest --runInBand --all",
        "test:dev": "jest --runInBand -o --verbose --watch",
        "build": "npm run lint && npm run tsc",
        "build:dev": "npm run tsc -- -w",
        "lint": "eslint src/**/*.ts --fix",
        "tsc": "shx rm -rf lib && shx mkdir lib && tsc"
