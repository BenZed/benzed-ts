    "bugs": {
        "url": "https://github.com/BenZed/benzed-ts/issues"
    },
    "homepage": "https://github.com/BenZed/benzed-ts#readme",
    "dependencies": {
        "@benzed/is": "^3.1.2",
        "@benzed/array": "^3.2.1",
        "@benzed/math": "^3.1.4",
        "@benzed/match": "^1.0.0",
        "@benzed/util": "^3.7.0",
        "@benzed/schema": "^0.3.0",
        "@benzed/feathers": "^1.0.0",
        "@benzed/renderer": "^0.1.0",
        "@benzed/immutable": "^3.1.3",
        "@benzed/fs": "^3.0.4",
        "socket.io-client": "^4.5.2",
