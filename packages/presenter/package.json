{
    "name": "@benzed/presenter",
    "description": "Markdown presentation components/app modules/website",
    "main": "./lib",
    "private": true,
    "version": "1.0.0",
    "scripts": {
        "test": "jest --run-in-band --all",
        "test:dev": "jest --run-in-band --only-changed --verbose",
        "lint": "eslint src/**/*.ts --fix",
        "build": "npm run lint && npm run tsc && npm run webpack",
        "create-public": "sh ./src/scripts/create-public.sh",
        "webpack": "npm run create-public && NODE_ENV=production webpack",
        "webpack:dev": "npm run create-public && webpack serve",
        "tsc": "shx rm -rf lib && shx mkdir lib && tsc",
        "serve": "node ./lib/scripts/serve.js",
        "serve:dev": "ts-node-dev ./src/scripts/serve.ts --watch ./src/app/*"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/BenZed/benzed-ts.git"
    },
    "author": "BenZed",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/BenZed/benzed-ts/issues"
    },
    "homepage": "https://github.com/BenZed/benzed-ts#readme",
    "dependencies": {
        "react": "^18.2.0",
        "react-markdown": "^8.0.6",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.9.0",
        "@benzed/app": "^1.0.0",
        "@benzed/fs": "^3.1.0",
        "@benzed/is": "^3.1.3",
        "@benzed/util": "^3.9.1",
        "@benzed/react": "^1.0.0",
        "@mantine/core": "^6.0.4",
        "@mantine/hooks": "^6.0.4",
        "@mantine/ssr": "^6.0.5",
        "@mantine/prism": "6.0.6",
        "@emotion/react": "11.10.6",
        "@tabler/icons-react": "2.14.0",
        "@benzed/math": "^3.1.4",
        "@benzed/string": "^3.2.3"
    }
}