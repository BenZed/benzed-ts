{
    "name": "@benzed/www",
    "description": "Website",
    "main": "./lib",
    "private": true,
    "version": "1.0.0",
    "scripts": {
        "test": "jest --run-in-band --all",
        "test:dev": "jest --run-in-band --only-changed --verbose",
        "lint": "eslint src/**/*.ts --fix",
        "build": "npm run lint && npm run tsc && npm run webpack",
        "webpack": "shx rm -rf public/ && NODE_ENV=production webpack",
        "webpack:dev": "shx rm -rf public/ && webpack serve",
        "tsc": "shx rm -rf lib && shx mkdir lib && tsc",
        "serve": "NODE_ENV=production node ./lib/scripts/serve.js",
        "serve:dev": "ts-node-dev ./src/scripts/serve.ts --watch ./src/app/*"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/BenZed/benzed-ts.git"
    },
    "author": "BenZed",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/BenZed/benzed-ts/issues"
    },
    "homepage": "https://github.com/BenZed/benzed-ts#readme",
    "dependencies": {
        "koa": "^2.14.1",
        "koa-static": "^5.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.9.0",
        "@benzed/app": "^1.0.0",
        "@benzed/logger": "^1.0.0",
        "@mantine/core": "^6.0.4",
        "@mantine/hooks": "^6.0.4",
        "@emotion/react": "11.10.6",
        "@benzed/react": "^1.0.0"
    },
    "devDependencies": {
        "@types/koa-static": "^4.0.2"
    }
}