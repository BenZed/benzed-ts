{
    "name": "@benzed/www",
    "description": "Website",
    "main": "./lib",
    "private": true,
    "version": "1.0.0",
    "scripts": {
        "test": "jest --run-in-band --all",
        "test:dev": "jest --run-in-band --only-changed --verbose",
        "lint": "eslint src/**/*.ts --fix",
        "build": "npm run lint && npm run tsc && npm run webpack",
        "webpack": "shx rm -rf public/ && webpack",
        "webpack:dev": "webpack serve",
        "tsc": "shx rm -rf lib && shx mkdir lib && tsc",
        "serve": "node ./lib/scripts/serve.js",
        "serve:dev": "ts-node-dev ./src/scripts/serve.ts --watch ./src/app/*"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/BenZed/benzed-ts.git"
    },
    "author": "BenZed",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/BenZed/benzed-ts/issues"
    },
    "homepage": "https://github.com/BenZed/benzed-ts#readme",
    "dependencies": {
        "styled-components": "^5.3.9",
        "react": "^18.2.0",
        "react-redux": "8.0.5",
        "@reduxjs/toolkit": "1.9.3",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.9.0",
        "@benzed/app": "^1.0.0",
        "@benzed/async": "^3.4.1",
        "@benzed/fs": "^3.1.0",
        "@benzed/react": "^1.0.0",
        "@benzed/util": "^3.9.1"
    },
    "devDependencies": {
        "@types/styled-components": "^5.1.26"
    }
}